import java.util.ArrayList;

public class SpecialCase extends NormalCase{
	private String name;
	private int age;
	
	public SpecialCase(String name, int age){
		this.name = name;
		this.age = -1;
	}
	
	public String toString(){
		return name + ": deceased";
	}
	
	public static void main(String[] args){
		ArrayList<NormalCase> customers = new ArrayList<NormalCase>();
		for(int i = 0 ; i < args.length ; i+= 2){
			String name = args[i];
			int age = args[i+1];
			NormalCase newCustomer = new NormalCase(name, age);
			customers.add(newCustomer);
		}
	}
	
}
