import java.util.Calendar;

public class ValueObject {
	private Calendar c;
	private int start;
	private int end;
	
	public ValueObject(int start, int end){
		c.set(Calendar.MONTH, start);
		c.set(Calendar.MONTH, end);
	}
	
	public int getStart(){
		return c.get(Calendar.MONTH);
	}
	
	public int getEnd(){
		return c.get(Calendar.MONTH);
	}
	
	public boolean equals(ValueObject a){
		if((this.start == a.start) & (this.end == a.end))
			return true;
		return false;
	}
}
